{
    "name": "erc8004-registry-parser",
    "version": "0.1.0",
    "description": "Validator and parser for ERC-8004 Trustless Agent Registration Files. Validates against the spec, classifies URI types, decodes raw event logs, and extracts metadata with comprehensive error reporting.",
    "main": "./src/index.mjs",
    "type": "module",
    "scripts": {
        "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest --detectOpenHandles",
        "test:coverage:src": "NODE_OPTIONS='--experimental-vm-modules' npx jest --detectOpenHandles --coverage --collectCoverageFrom='src/**/*.mjs'",
        "test:file": "NODE_OPTIONS='--experimental-vm-modules' npx jest --detectOpenHandles"
    },
    "keywords": [
        "erc-8004",
        "trustless-agents",
        "ethereum",
        "validator",
        "mcp",
        "a2a"
    ],
    "license": "MIT",
    "dependencies": {
        "ethers": "^6.14.0"
    },
    "devDependencies": {
        "a2a-agent-validator": "github:FlowMCP/a2a-agent-validator#91d360fad6fa9ccdecb007273b61456de37be218",
        "jest": "^30.0.5",
        "x402-mcp-validator": "github:FlowMCP/x402-mcp-validator#6095f08ba98f0b69d66a31a9b2ebd3bd7fff0558"
    }
}
